<resources>
    
    <!-- 破碎聚合TextView -->
    <declare-styleable name="ReunionTextView">
        <attr name="rt_in_color" format="color" />
        <attr name="rt_tv_width" format="dimension" />
        <attr name="rt_tv_height" format="dimension" />
        <attr name="rt_tv_size" format="dimension" />
        <attr name="rt_tv_content" format="string" />
    </declare-styleable>
    

    <!-- 进度显示TextView -->
    <declare-styleable name="ProgressTextView">
        <attr name="in_color" format="color" />
        <attr name="tv_width" format="dimension" />
        <attr name="tv_height" format="dimension" />
        <attr name="tv_size" format="dimension" />
        <attr name="progress_step" format="float" />
        <attr name="tv_content" format="string" />
    </declare-styleable>

</resources>